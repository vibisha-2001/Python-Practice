    "a=int(input(\"Enter a num:\"))\n",
    "if a<=1:\n",
    "    print(\"Not a prime\")\n",
    "else:\n",
    "    is_prime=True\n",
    "    for i in range(2,a):\n",
    "        if a%i==0:\n",
    "            is_prime=False\n",
    "            break\n",
    "    if is_prime:\n",
    "        print(f\"{a} is a Prime number\")\n",
    "    else:\n",
    "        print(f\"{a} is not a prime\")\n",
